local function UDGMQ_fake_script() -- TOH.Script 
	local script = Instance.new('Script', TOH)

	while wait() do
		if script.Parent.Parent.TOH.Visible == true then
			script.Parent.Parent.TOH:TweenPosition(UDim2.new(0.499, 0,0.04, 0))
		end
	end
end
coroutine.wrap(UDGMQ_fake_script)()
