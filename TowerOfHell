-- Scripts:

local function FUYKP_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	-- put this inside close with a localscript
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.ClipsDescendants = true
		script.Parent.Parent:TweenSize(UDim2.new(0,0,0,0),"In","Linear",0.3,false,nil)
	end)
end
coroutine.wrap(FUYKP_fake_script)()
local function YLAER_fake_script() -- TextButton_2.Script 
	local script = Instance.new('Script', TextButton_2)

	-- put this in minimize as a localscript
	local on = false
	local old 
	script.Parent.MouseButton1Click:Connect(function()
	
	
		if on == false then
			on = true
			old = script.Parent.Parent.Position
			script.Parent.Parent:TweenPosition(UDim2.new(0.519, 0,0.94, 0),"Out","Linear",0.3,false,nil)
			game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {Rotation = 180}):Play()
	
		else
			on = false
			script.Parent.Parent:TweenPosition(old,"Out","Linear",0.3,false,nil)
			game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {Rotation = 0}):Play()
		end
		print(old)
	end)
end
coroutine.wrap(YLAER_fake_script)()
local function DRVI_fake_script() -- God.Script 
	local script = Instance.new('Script', God)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(26, 30, 68)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(77, 77, 77)}):Play()
	end)
	
	script.Parent.MouseButton1Down:Connect(function()
		while wait() do
			while wait() do
				-- God Mode script
				-- Defining Variables
				-- Uwu
	
				local Player = game.Players.LocalPlayer
				local PlayerScripts = Player.PlayerScripts
	
				local KillScript = Player.Character.KillScript
	
				-- Disabling it
	
				for i,v in pairs(getconnections(KillScript.Changed)) do
					v:Disable()
				end
	
				-- Removing it
	
				KillScript:Remove()
			end
		end
	end)
end
coroutine.wrap(DRVI_fake_script)()
local function WHPPBA_fake_script() -- Antiban.Script 
	local script = Instance.new('Script', Antiban)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(26, 30, 68)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(77, 77, 77)}):Play()
	end)
	
	script.Parent.MouseButton1Down:Connect(function()
		while wait() do
			-- Disable anticheat
			-- Defining the Variables
			-- SantaTraked :3
	
			local Player = game.Players.LocalPlayer
			local PlayerScripts = Player.PlayerScripts
	
			local Anti1 = PlayerScripts.LocalScript
			local Anti2 = PlayerScripts.LocalScript2
	
			-- Disabling the AntiCheat
	
			for i,v in pairs(getconnections(Anti1.Changed)) do
				v:Disable()
			end
	
			for i,v in pairs(getconnections(Anti2.Changed)) do
				v:Disable()
			end
	
			-- Removing the Anticheat COMPLETELY
	
			Anti1:Remove()
			Anti2:Remove()
		end
	end)
end
coroutine.wrap(WHPPBA_fake_script)()
local function FLHVLY_fake_script() -- TpEnd.Script 
	local script = Instance.new('Script', TpEnd)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(26, 30, 68)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(77, 77, 77)}):Play()
	end)
	
	script.Parent.MouseButton1Down:Connect(function()
			local plr = game.Players.LocalPlayer
			local char = plr.Character or plr.CharacterAdded:Wait() -- gets the character, or waits until it exists.
	
			local function tptopart(part)
				local hrp = char:FindFirstChild('HumanoidRootPart') -- get the HumanoidRootPart, which is the main part of the character that controls it's position and CFrame.
				if not hrp then return end -- if they don't have a HumanoidRootPart, cancel the function.
				for i = 0,1,0.01 do
					hrp.CFrame = hrp.CFrame:Lerp(part.CFrame,i)
					game:GetService('RunService').RenderStepped:Wait()
				end
			end
	
			-- Example of how to use:
			local part = workspace.tower.sections.finish.FinishGlow
			tptopart(part)
		end)
end
coroutine.wrap(FLHVLY_fake_script)()
local function CHSJC_fake_script() -- Giver.Script 
	local script = Instance.new('Script', Giver)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(26, 30, 68)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(77, 77, 77)}):Play()
	end)
	
	script.Parent.MouseButton1Down:Connect(function()
			-- Defining general Variables
	
			local ReplicatedStorage = game.ReplicatedStorage
			local Gear = ReplicatedStorage.Gear
			local Players = game.Players
			local LocalPlayer = Players.LocalPlayer
			local Backpack = LocalPlayer.Backpack
	
			-- Defining tool Variables
	
			local SpeedCoil = Gear.speed
			local Trowel = Gear.trowel
			local Fusion = Gear.fusion
			local Gravity = Gear.gravity
			local JumpCoil = Gear.jump
			local Grapple = Gear.hook
			local HourGlass = Gear.hourglass
			local Bomb = Gear.bomb
	
			-- Defining clone Variables
	
			local SpeedCoilClone = SpeedCoil:Clone()
			local TrowelClone = Trowel:Clone()
			local FusionClone = Fusion:Clone()
			local GravityClone = Gravity:Clone()
			local JumpCoilClone = JumpCoil:Clone()
			local GrappleClone = Grapple:Clone()
			local HourGlassClone = HourGlass:Clone()
			local BombClone = Bomb:Clone()
	
			-- Giving the tools
	
			SpeedCoilClone.Parent = Backpack
			TrowelClone.Parent = Backpack
			FusionClone.Parent = Backpack
			GravityClone.Parent = Backpack
			JumpCoilClone.Parent = Backpack
			GrappleClone.Parent = Backpack
			HourGlassClone.Parent = Backpack
			BombClone.Parent = Backpack
	
		end)
end
coroutine.wrap(CHSJC_fake_script)()
local function ORCQXQE_fake_script() -- ProTowers.Script 
	local script = Instance.new('Script', ProTowers)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(26, 30, 68)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(77, 77, 77)}):Play()
	end)
	
	script.Parent.MouseButton1Down:Connect(function()
		local plr = game.Players.LocalPlayer
		local char = plr.Character or plr.CharacterAdded:Wait() -- gets the character, or waits until it exists.
	
		local function tptopart(part)
			local hrp = char:FindFirstChild('HumanoidRootPart') -- get the HumanoidRootPart, which is the main part of the character that controls it's position and CFrame.
			if not hrp then return end -- if they don't have a HumanoidRootPart, cancel the function.
			for i = 0,1,0.01 do
				hrp.CFrame = hrp.CFrame:Lerp(part.CFrame,i)
				game:GetService('RunService').RenderStepped:Wait()
			end
		end
	
		-- Example of how to use:
		local part = workspace.tower.sections.start.gate.gate
		tptopart(part)
	end)
end
coroutine.wrap(ORCQXQE_fake_script)()
local function TCSUD_fake_script() -- TextButton_3.Script 
	local script = Instance.new('Script', TextButton_3)

	-- put this inside the button as a localscript
	-- also change the colors to whatever you chose.. these are my colors
	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(26, 30, 68)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(77, 77, 77)}):Play()
	end)
	script.Parent.MouseButton1Down:Connect(function()
	
		script.Parent.Parent.Parent.Parent.Parent.Settings:TweenPosition(UDim2.new(0.499, 0,0.1, 0), nil, nil, 1.5)
		wait()
		script.Parent.Parent.Parent.Parent.Parent.TOH:TweenPosition(UDim2.new(2.499, 0,0.1, 0),nil, nil, 1.5)
	end)
end
coroutine.wrap(TCSUD_fake_script)()
